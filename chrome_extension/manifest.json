{
    "name": "TimeTrove Extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "service_worker.js"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "scripting",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "img/icon.png"
    }
  }